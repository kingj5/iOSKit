#tag Class
Protected Class AVCaptureInput
Inherits NSObject
	#tag ComputedProperty, Flags = &h0
		#tag Getter
			Get
			  declare function ports_ lib AVFoundationLib selector "ports" (obj_id as ptr) as ptr
			  Return new NSArray(ports_(self))
			End Get
		#tag EndGetter
		ports As Foundation.NSArray
	#tag EndComputedProperty


	#tag ViewBehavior
		#tag ViewProperty
			Name="Index"
			Visible=true
			Group="ID"
			InitialValue="-2147483648"
			Type="Integer"
		#tag EndViewProperty
